@inherits Umbraco.Web.Mvc.UmbracoViewPage<ITitleAndContent>
@using Umbraco.Web
@{
    var multiLinks = Umbraco.AssignedContentItem.GetProperty("tcCTA").AsEnumerableOfOne();
}
<section id="one">
    <div class="inner">
        <header>
           <h2>@Model.TcHeading</h2>
        </header>
        <p>@Model.TcContent</p>
        <ul class="actions">
            @foreach (var item in multiLinks)
            {
                IEnumerable<Umbraco.Web.Models.Link> links = (IEnumerable<Umbraco.Web.Models.Link>)item.Value(); 
                foreach (var link in links)
                {
                    <li><a href="@link.Url" class="button alt">@link.Name</a></li>
                }
            }
        </ul>
    </div>
</section>
